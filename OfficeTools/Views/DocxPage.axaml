<UserControl
    x:Class="OfficeTools.Views.DocxPage"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:u="https://irihi.tech/ursa"
    xmlns:vm="clr-namespace:OfficeTools.ViewModels"
    d:DesignHeight="450"
    d:DesignWidth="800"
    x:DataType="vm:DocxPageViewModel"
    mc:Ignorable="d">
    <Grid RowDefinitions="*,Auto">
        <HeaderedContentControl Header="xxxx">
            <ListBox
                u:ScrollTo.ButtonTheme="{DynamicResource PrimaryScrollToButton}"
                u:ScrollTo.Direction="Top"
                ItemsSource="{Binding ListStringItems}"
                ScrollViewer.HorizontalScrollBarVisibility="Disabled"
                ScrollViewer.VerticalScrollBarVisibility="Auto" />
        </HeaderedContentControl>

        <TextBlock Grid.Row="1">
            11111111111111111111111111
        </TextBlock>
    </Grid>

</UserControl>